{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "port": 9222,
            "url": "http://localhost:4200/",
            "webRoot": "${workspaceRoot}/index.html"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Attach to Chrome",
            "port": 9222,
            "sourceMaps": false,
            "webRoot": "${workspaceRoot}/index.html"
        }
    ]
}